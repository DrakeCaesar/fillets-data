name frkavec
var okno
var faze
var cinnost
var delay
var strana
ini
cinnost:=0;
spec:=11;
67 Prg2
case okno of
  1:x:=kabina.x+2;
  2:x:=kabina.x+8;
end;
if cinnost>0 then spec:=strana*10
else spec:=11;
  
case cinnost of
  0:if random(1000)<8 then
      begin
        okno:=random(2)+1;
        if globpole[okno]=0 then 
          begin
            cinnost:=1;
            faze:=0;
            strana:=random(2);
            globpole[okno]:=1;
          end;
      end;
  1:case faze of
      0..4:
        begin
          afaze:=faze;
          inc(faze);
        end;
      5:begin
          afaze:=5;
          delay:=random(10)+5;
          inc(faze);
        end;
      6:if delay>0 then dec(delay)
        else 
          begin
            afaze:=6;
            delay:=10;
            inc(faze);
            {snd}
          end;
      7:if delay>0 then dec(delay)
        else 
          begin
            afaze:=5;
            if random(100)<75 then 
              begin
                delay:=2+random(10);
                faze:=6;
              end
            else inc(faze);
          end;
      8:inc(faze);
      9:begin
          globpole[okno]:=0;
          inc(faze);
        end;
      10..14:
        begin
          afaze:=14-faze;
          inc(faze);
          if faze=15 then cinnost:=0;
        end;
    end;
end;               
            

          
      
     
