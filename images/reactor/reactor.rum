var smutny
ini
vlnit:=0;
del:=0;
ocko:=0;
smer:=0;
faze:=0;
smutny:=0;
71 Prg2
case dir of
  dir_no:if vlnit=-1 then vlnit:=8;
  dir_down:vlnit:=-1;
  else vlnit:=8;
end;

if vlnit>0 then smutny:=0;


if vlnit>0 then
 if del=0 then
  begin
    case vlnit of
      8,7,6:del:=1;
      5,4,3:del:=2;
      else del:=3;
    end;
    if random(2)=0 then
      afaze:=(afaze+1)mod 4
    else
      afaze:=(afaze+3)mod 4;
    dec(vlnit);
    if vlnit=0 then del:=0;
    if vlnit=0 then afaze:=0
    else if vlnit=1 then afaze:=3;
  end
 else
  dec(del)
else if smutny>0 then
  begin
    if ocko=0 then
      if random(100)<10 then ocko:=3;
    if ocko>0 then dec(ocko);
    if ocko>0 then afaze:=15
    else afaze:=14;
    dec(smutny);
  end
else
  begin
    if random(100)<10 then smer:=1-smer;
    case faze of
      0:begin
          afaze:=0;
          if random(100)<10 then faze:=1;
        end;
    1,4:begin
          inc(faze);
          afaze:=4;
        end;
    2,3:begin
          inc(faze);
          afaze:=5;
        end;
     5:begin
          afaze:=0;
          faze:=0;
        end;
    end;
    case afaze of
      0:if smer=1 then afaze:=6;
      4:if smer=1 then afaze:=7;
    end;

    if ocko=0 then
      if random(100)<10 then ocko:=3;
    if ocko>0 then dec(ocko);

    if ocko>0 then
      if afaze=0 then afaze:=9
      else afaze:=afaze+6;
  end;
