name ocicko
var cinnost
var faze
var citac
ini
cinnost:=0;
78 Prg2
case cinnost of
  0:begin
      if random(100)<10 then
        case random(8) of
          0..2:
            begin
              citac:=random(5)+5;
              cinnost:=1;
              faze:=random(2)*2;
            end;
          3:begin
              citac:=random(3)+2;
              cinnost:=2;
              faze:=random(2)*2;
            end;
          4..6:
            begin
              citac:=random(12)+12;
              cinnost:=3+random(2);
            end;
          7:begin
              citac:=random(10)+2;
              cinnost:=5;
            end;
        end; 
    end;
  1,2:
    begin
      case faze of
        0:begin
            if cinnost=1 then afaze:=1
                         else afaze:=3;
            if random(100)<20 then inc(faze);
          end;
        1:begin
            afaze:=0;
            inc(faze);
          end;
        2:begin
            if cinnost=1 then afaze:=2
                         else afaze:=4;
            if random(100)<20 then inc(faze);
          end;
        3:begin
            afaze:=0;
            dec(citac);
            if citac=0 then cinnost:=0
                       else faze:=0;
          end;
      end; 
    end;
  3,4,5:
    begin
      case cinnost of
        3:case afaze of
            0:afaze:=random(4)+1;
            1:afaze:=3;
            2:afaze:=4;
            3:afaze:=2;
            4:afaze:=1;
          end;
        4:case afaze of
            0:afaze:=random(4)+1;
            1:afaze:=4;
            2:afaze:=3;
            3:afaze:=1;
            4:afaze:=2;
          end;
        5:if random(100)<40 then afaze:=random(5);
      end;
      dec(citac);
      if citac=0 then 
        begin
          cinnost:=0;
          afaze:=0;
        end;
    end;
end;               
